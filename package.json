{
  "name": "sptraderb",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "typecheck": "tsc --noEmit",
    "check-all": "npm run typecheck && npm run lint",
    "errors": "node check-errors.js",
    "deploy": "npm update sptrader-chart-lib && git add -A && git commit -m 'update chart lib' && git push"
  },
  "dependencies": {
    "@mantine/charts": "^8.1.0",
    "@mantine/core": "^8.1.0",
    "@mantine/dates": "^8.1.0",
    "@mantine/form": "^8.1.0",
    "@mantine/hooks": "^8.1.0",
    "@mantine/notifications": "^8.1.0",
    "@monaco-editor/react": "^4.7.0",
    "@tabler/icons-react": "^3.34.0",
    "dayjs": "^1.11.13",
    "lightweight-charts": "^5.0.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.6.2",
    "sptrader-chart-lib": "^2.0.19",
    "talib-web": "^0.1.3",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@types/lodash": "^4.17.20",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "prettier": "^3.6.2",
    "typescript": "~5.6.2",
    "vite": "^6.0.3"
  }
}
