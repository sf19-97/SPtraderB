# Market Data Server (for fetching historical candles)
WS_MARKET_DATA_URL=https://ws-market-data-server.fly.dev

# Database (optional - can use filesystem instead)
DATABASE_URL=postgres://postgres:password@localhost:5432/sptraderb

# Redis (optional - for caching backtest results)
REDIS_URL=redis://localhost:6379

# Server
PORT=3001

# CORS (comma-separated allowed origins)
ALLOWED_ORIGINS=http://localhost:5173,http://localhost:3000

# Logging
RUST_LOG=info,sptraderb_api=debug
